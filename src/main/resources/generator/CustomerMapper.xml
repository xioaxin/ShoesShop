    <result column="customerAddr" jdbcType="VARCHAR" property="customeraddr" />
    <result column="customerPostNumber" jdbcType="VARCHAR" property="customerpostnumber" />
    <result column="customerEmail" jdbcType="VARCHAR" property="customeremail" />
